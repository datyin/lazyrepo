{
  "name": "@lazypack/naive-ui",
  "type": "module",
  "version": "0.0.1",
  "description": "Naive UI component package",
  "author": "Jove Jonovski <jovejonovski@gmail.com> (https://github.com/datyin)",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "dev": "tsdown --watch",
    "build": "pnpm run lint && pnpm run typecheck && tsdown"
  },
  "peerDependencies": {
    "@vueuse/core": ">=13.8",
    "naive-ui": ">=2.42",
    "vue": ">=3.5",
    "vue-router": ">=4.5"
  },
  "devDependencies": {
    "@iconify-json/fluent": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "import": "dist/index.js",
        "require": "dist/index.cjs",
        "types": "dist/index.d.ts"
      }
    }
  }
}
